else
  export NODE_PATH="/Users/alex/code/Personal_Projects/web3-social-app/node_modules/.pnpm/hardhat@2.24.3_bufferutil@4.0.9_ts-node@10.9.2_@types+node@20.19.1_typescript@5.8.3__typescri_cjidfwsx7lc7ttelp7v6cz5vc4/node_modules/hardhat/internal/cli/node_modules:/Users/alex/code/Personal_Projects/web3-social-app/node_modules/.pnpm/hardhat@2.24.3_bufferutil@4.0.9_ts-node@10.9.2_@types+node@20.19.1_typescript@5.8.3__typescri_cjidfwsx7lc7ttelp7v6cz5vc4/node_modules/hardhat/internal/node_modules:/Users/alex/code/Personal_Projects/web3-social-app/node_modules/.pnpm/hardhat@2.24.3_bufferutil@4.0.9_ts-node@10.9.2_@types+node@20.19.1_typescript@5.8.3__typescri_cjidfwsx7lc7ttelp7v6cz5vc4/node_modules/hardhat/node_modules:/Users/alex/code/Personal_Projects/web3-social-app/node_modules/.pnpm/hardhat@2.24.3_bufferutil@4.0.9_ts-node@10.9.2_@types+node@20.19.1_typescript@5.8.3__typescri_cjidfwsx7lc7ttelp7v6cz5vc4/node_modules:/Users/alex/code/Personal_Projects/web3-social-app/node_modules/.pnpm/node_modules:$NODE_PATH"
