#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/alex/code/Personal_Projects/web3-social-app/node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.24.3_bufferutil@4.0.9_ts-node@10.9.2_@types+node@20.19.1_t_lxjhi34o5sx5mn2nuy572g5e6a/node_modules/solidity-coverage/plugins/node_modules:/Users/alex/code/Personal_Projects/web3-social-app/node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.24.3_bufferutil@4.0.9_ts-node@10.9.2_@types+node@20.19.1_t_lxjhi34o5sx5mn2nuy572g5e6a/node_modules/solidity-coverage/node_modules:/Users/alex/code/Personal_Projects/web3-social-app/node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.24.3_bufferutil@4.0.9_ts-node@10.9.2_@types+node@20.19.1_t_lxjhi34o5sx5mn2nuy572g5e6a/node_modules:/Users/alex/code/Personal_Projects/web3-social-app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/alex/code/Personal_Projects/web3-social-app/node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.24.3_bufferutil@4.0.9_ts-node@10.9.2_@types+node@20.19.1_t_lxjhi34o5sx5mn2nuy572g5e6a/node_modules/solidity-coverage/plugins/node_modules:/Users/alex/code/Personal_Projects/web3-social-app/node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.24.3_bufferutil@4.0.9_ts-node@10.9.2_@types+node@20.19.1_t_lxjhi34o5sx5mn2nuy572g5e6a/node_modules/solidity-coverage/node_modules:/Users/alex/code/Personal_Projects/web3-social-app/node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.24.3_bufferutil@4.0.9_ts-node@10.9.2_@types+node@20.19.1_t_lxjhi34o5sx5mn2nuy572g5e6a/node_modules:/Users/alex/code/Personal_Projects/web3-social-app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.24.3_bufferutil@4.0.9_ts-node@10.9.2_@types+node@20.19.1_t_lxjhi34o5sx5mn2nuy572g5e6a/node_modules/solidity-coverage/plugins/bin.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.24.3_bufferutil@4.0.9_ts-node@10.9.2_@types+node@20.19.1_t_lxjhi34o5sx5mn2nuy572g5e6a/node_modules/solidity-coverage/plugins/bin.js" "$@"
