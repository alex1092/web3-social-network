{
  "name": "@web3-social/contracts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "compile": "hardhat compile",
    "test": "hardhat test",
    "deploy:testnet": "hardhat run scripts/deploy.ts --network arbitrumSepolia",
    "deploy:mainnet": "hardhat run scripts/deploy.ts --network arbitrumOne",
    "verify": "hardhat run scripts/verify.ts",
